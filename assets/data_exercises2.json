[
    {
      "routineName": "RUTINA DE INTENSIDAD MODERADA",
      "routineSteps": [
        {
          "stepNumber": 1,
          "stepDescription": "Extensión de la columna cervical (estiramiento del cuello metiendo barbilla adentro y arriba).",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F1.png?alt=media&token=d165fceb-59c8-4525-a5d5-ce471689f0f8"
        },
        {
          "stepNumber": 2,
          "stepDescription": "Rotación de la cabeza a ambos lados, despacio y sin dolor.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F2.png?alt=media&token=7a91591b-4a58-4712-a892-51b5d8cb3e22"
        },
        {
          "stepNumber": 3,
          "stepDescription": "Elevar y descender hombros, acompañando los movimientos con inspiraciones y expiraciones lentas y profundas.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F3.png?alt=media&token=4277f1ee-ca3e-443a-9763-3296e934d00e"
        },
        {
          "stepNumber": 4,
          "stepDescription": "Hombros adelante y atrás.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F4.png?alt=media&token=6f09f3ff-e00a-46d8-a55c-af5582d5adca"
        },
        {
          "stepNumber": 5,
          "stepDescription": "Respiraciones diafragmáticas profundas y lentas (hinchar/deshinchar la barriga). Manos sobre el ombligo.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F5.png?alt=media&token=8b3227c7-4785-4ff6-9482-b205f8b1b16b"
        },
        {
          "stepNumber": 6,
          "stepDescription": "Elevación de brazos: primero a la horizontal, luego arriba y volvemos a empezar.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F6.png?alt=media&token=85f6496e-604f-426f-82e3-857d2ed2466b"
        },
        {
          "stepNumber": 7,
          "stepDescription": "Rotaciones externas de hombro con los brazos en abducción y flexionados; como si tocara los platillos.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F7.png?alt=media&token=f0070370-0645-4840-a218-b11f4cbf2871"
        },
        {
          "stepNumber": 8,
          "stepDescription": "Realizar circunferencias con las manos, adelante y atrás; empezando por una amplitud reducida e ir aumentándola progresivamente.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F8.png?alt=media&token=bdef1edb-1f91-4d92-86a5-9fed05c41546"
        },
        {
          "stepNumber": 9,
          "stepDescription": "Girando desde la cintura con una banda imaginaria.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F9.png?alt=media&token=afe7eee1-6cd6-4ad6-a91d-20b909a3a3a9"
        },
        {
          "stepNumber": 10,
          "stepDescription": "Llevar una cinta imaginaria arriba, atrás y mano adelante y volver a empezar.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F10.png?alt=media&token=027a0ebf-badc-4959-93c2-94aff2ec1d36"
        },
        {
          "stepNumber": 11,
          "stepDescription": "Flexo-extensión de brazos elevando alternativamente una de las piernas.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F11.png?alt=media&token=2b24275c-92ef-428c-9eb7-7247e3affa05"
        },
        {
          "stepNumber": 12,
          "stepDescription": "Extensión de rodilla manteniendo 5-6 segundos.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F12.png?alt=media&token=38602d4e-2f0d-4f41-a18a-24e2cc880c0f"
        },
        {
          "stepNumber": 13,
          "stepDescription": "Coger una pierna por la parte posterior del muslo y llevarla hacia el tronco llevando la punta del pie hacia la rodilla.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F13.png?alt=media&token=8facc76e-a5b0-4029-9ea7-d64b12d81736"
        },
        {
          "stepNumber": 14,
          "stepDescription": "Fortalecimiento de cuádriceps: poner un pie detrás del otro y ejercer fuerza en sentido contrario hasta extender las rodillas.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F14.png?alt=media&token=5a0657b5-6381-48a0-b4a9-5d8851084a4e"
        },
        {
          "stepNumber": 15,
          "stepDescription": "Marcar el ritmo de la música con los pies, abriendo y cerrando puños.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F15.png?alt=media&token=2e6d0e51-5d43-442f-9056-bb098a9ffd9a"
        },
        {
          "stepNumber": 16,
          "stepDescription": "Letra “O” con los dedos y simultáneamente, realizamos punta talón con los pies.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F16.png?alt=media&token=299a7ef5-4f1d-4486-a58b-6cbe9a4f701b"
        },
        {
          "stepNumber": 17,
          "stepDescription": "Transfiera el peso del cuerpo de un glúteo, levantando la rodilla",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F17.png?alt=media&token=ffcc04ca-eced-4c5d-b577-adbeddd15d90"
        },
        {
          "stepNumber": 18,
          "stepDescription": "Transfiera el peso a un glúteo y flexione la rodilla y la cadera hacia el tronco primero, y extendiendo la rodilla, después.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F18.png?alt=media&token=51b68cb9-3996-42a3-97d3-bf4abf0f81de"
        },
        {
          "stepNumber": 19,
          "stepDescription": "Sentado, extensión de la rodilla y realizar al menos 3 círculos con el pie.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F19.png?alt=media&token=40588ddc-a622-43a1-9921-c25e619d501e"
        },
        {
          "stepNumber": 20,
          "stepDescription": "Caminar pero sin desplazarse y coordinando brazos y piernas (con música).",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F20.png?alt=media&token=2d4a2ef3-f8f6-4d23-9b90-36c6ee3704e7"
        },
        {
          "stepNumber": 21,
          "stepDescription": "Levantar los talones atrás flexionado la rodilla. Alternar con ambas piernas.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F21.png?alt=media&token=12c7a689-b571-48ef-92de-1ed4bfb3c28c"
        },
        {
          "stepNumber": 22,
          "stepDescription": "Inspiramos profunda y lentamente al tiempo que elevamos los brazos hasta la horizontal. A continuación expiramos lentamente descendiendo los brazos.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F22.png?alt=media&token=b65845e5-7d5e-4f43-87f9-71e47baca3ad"
        },
        {
          "stepNumber": 23,
          "stepDescription": "Dar tres pasos, flexionando ligeramente las rodillas y dar un aplauso.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F23.png?alt=media&token=b06890bb-1b26-422c-992b-f26e23bbb66c"
        },
        {
          "stepNumber": 24,
          "stepDescription": "Bailar al Twist.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F24.png?alt=media&token=ea2570eb-adea-45bc-9e75-009a41411bd7"
        },
        {
          "stepNumber": 25,
          "stepDescription": "Flexionar las caderas levantando las rodillas y tocarlas con las manos.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F25.png?alt=media&token=c5110098-a3a1-4726-a31f-4d54b259c359"
        },
        {
          "stepNumber": 26,
          "stepDescription": "Imaginar que estamos dirigiendo una orquesta. Realizar diversidad de movimientos de forma libre (apoyo musical).",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F26.png?alt=media&token=952e380d-b30f-45c5-b73c-ecc7e3ff95d4"
        },
        {
          "stepNumber": 27,
          "stepDescription": "Realizar movimientos imaginando que tocamos un piano.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F27.png?alt=media&token=61c47156-3f55-413c-aa03-a32b2df6bfef"
        },
        {
          "stepNumber": 28,
          "stepDescription": "Tocar, alternativamente, con el codo en la rodilla opuesta.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F28.png?alt=media&token=812cc77d-95b7-4421-9267-ca5dd4a700c3"
        },
        {
          "stepNumber": 29,
          "stepDescription": "Dar tres pasos laterales cruzando una pierna frente a la otra y aplaudir en el cuarto paso.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F29.png?alt=media&token=952ac2ca-f2c0-4200-a114-d3b6a1fe7082"
        },
        {
          "stepNumber": 30,
          "stepDescription": "Anteversión y retroversión de la pelvis (báscula pélvica).",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F30.png?alt=media&token=b5268be0-afe3-44a8-8618-6a6d48133443"
        },
        {
          "stepNumber": 31,
          "stepDescription": "Estiramiento de la de la zona lumbar y parte posterior del muslo.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F31.png?alt=media&token=6f270e38-f418-47f3-bc31-3fdbc19919e1"
        },
        {
          "stepNumber": 32,
          "stepDescription": "Mover las piernas de un lado al otro.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F32.png?alt=media&token=ebd321e2-bf9c-4a82-a53a-22d2b0041bcb"
        },
        {
          "stepNumber": 33,
          "stepDescription": "Acostado: girando desde la cintura con los brazos extendidos horizontalmente (estiramiento de glúteos y cintura).",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F33.png?alt=media&token=6a727073-08d8-4b23-b327-f74294404337"
        },
        {
          "stepNumber": 34,
          "stepDescription": "Flexibilización de la espalda.",
          "stepImage": "https://firebasestorage.googleapis.com/v0/b/authnutritionaldiabetesapp.appspot.com/o/Moderate_routine%2F34.png?alt=media&token=79ac3da4-fe19-421e-9142-537afb157ed2"
        }
      ]
    }
  ]
  